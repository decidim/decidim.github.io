---
nav: trademark
description:
---

<section>
  <div class="container pt-6 pb-6 md:pt-24 md:pb-12">
    <div class="md:w-3/5 space-y-6">
      <h1 class="h1 md:whitespace-nowrap">
        <%= t("trademark.title") %>
      </h1>
    </div>
  </div>
</section>

<section>
  <div class="container pt-12 pb-20">

    <% (1..3)&.each do |n| %>
      <div class="par-md mt-4 prose prose-xl"><%= t("trademark.p#{n}") %></div>
    <% end %>

    <div class="par-md mt-4 prose prose-xl">
      <ol class="list-decimal par-md">
        <% (1..3)&.each do |n| %>
          <li><%= t("trademark.goals.l#{n}") %></li>
        <% end %>
      </ol>
    </div>

    <% (1..3)&.each do |n| %>
      <h5 class="h5 first:mt-0 mt-8"><%= t("trademark.examples-#{n}.title") %></h5>
      <div class="par-md mt-4 prose prose-xl">
        <ul class="list-disc par-md text-gray-500 prose">
          <% (1..3)&.each do |i| %>
            <% unless t("trademark.examples-#{n}.l#{i}").start_with?("translation missing") %>
              <li><%= t("trademark.examples-#{n}.l#{i}") %></li>
            <% end %>
          <% end %>
        </ul>
      </div>
    <% end %>

    <div class="par-md mt-4 prose prose-xl"><%= t("trademark.p4") %></div>
    <div class="par-md mt-4 prose prose-xl"><%= I18n.t("trademark.p5", rails_url: "https://rubyonrails.org/trademarks", wordpress_url: "https://wordpressfoundation.org/trademark-policy/") %></div>
  </div>
</section>

